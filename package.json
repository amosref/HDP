             },
    "homepage":  "https://github.com/8400TheHealthNetwork/ILCore-IG#readme",
    "dependencies":  {
                         "adm-zip":  "^0.5.14",
                         "axios":  "^1.7.2",
                         "execa":  "^9.3.0",
                         "fs-extra":  "^11.2.0",
                         "js-yaml":  "^4.1.0",
                         "jsonata":  "^2.0.5",
                         "path":  "^0.12.7",
                         "tar":  "^7.4.0",
                         "fsh-sushi":  "^3.17.0"
                     },
    "devDependencies":  {
                            "fsh-and-chips":  "git+https://github.com/Outburn-IL/fhir-ig-tools.git"
                        }
}
